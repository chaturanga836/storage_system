{
  "ingestion": {
    "port": 8001,
    "max_connections": 1000,
    "batch_size": 1000,
    "flush_interval": "5s"
  },
  "query": {
    "port": 8081,
    "max_connections": 1000,
    "cache_size": 1000,
    "timeout": "30s"
  },
  "data_processor": {
    "workers": 4,
    "compaction_interval": "10m",
    "batch_size": 1000
  },
  "storage": {
    "data_dir": "./data",
    "compression": "snappy",
    "block_size": 65536
  },
  "wal": {
    "dir": "./wal",
    "segment_size": 67108864,
    "sync_writes": true
  },
  "auth": {
    "enabled": false,
    "jwt_secret": "dev-secret-key",
    "token_ttl": "24h"
  },
  "catalog": {
    "db_path": "./catalog.db",
    "cache_size": 1000
  }
}
