{
  "catalogs": [
    {
      "user": "admin123",
      "catalog": ".*",
      "allow": "all"
    }
  ],
  "schemas": [
    {
      "user": "admin123",
      "catalog": ".*",
      "owner": true
    }
  ],
  "tables": [
    {
      "user": "admin123",
      "catalog": "nessie",
      "schema": ".*",
      "table": ".*",
      "privileges": [
        "SELECT",
        "INSERT",
        "DELETE",
        "UPDATE",
        "OWNERSHIP"
      ]
    }
  ],
  "functions": [
    {
      "user": "admin123",
      "catalog": ".*",
      "function": ".*",
      "privileges": [
        "EXECUTE",
        "GRANT_EXECUTE"
      ]
    }
  ],
  "queries": [
    {
        "user": "admin123",
        "allow": ["execute", "view", "kill"]
    }
  ]
}
